{
  "name": "gleez/gravatar",
  "type": "library",
  "description": "A lightweight PHP library providing easy gravatar integration",
  "keywords": [
    "gravatar",
    "avatar",
    "gleez"
  ],
  "homepage": "https://github.com/sergeyklay/gleez-gravatar/releases",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gleez Team",
      "email": "info@gleezcms.org",
      "homepage": "http://gleezcms.org",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@gleez.com",
    "issues": "https://github.com/sergeyklay/gleez-gravatar/issues",
    "source": "https://github.com/sergeyklay/gleez-gravatar"
  },
  "suggest": {
    "gleez/autoloader": "PSR-4 compliant autoloader for PHP 5.3 or higher"
  },
  "require": {
    "php": ">=5.3.29"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Gleez\\Gravatar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    "Gleez\\Tests\\Gravatar\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
